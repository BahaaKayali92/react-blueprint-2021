{
    "plugins": [
      "babel",
      "react"
    ],
    "extends": "airbnb",
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "arrowFunctions": true,
      "binaryLiterals": false,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": false,
      "generators": false,
      "modules": false,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": false,
      "regexUFlag": false,
      "regexYFlag": false,
      "restParams": true,
      "spread": true,
      "superInFunctions": false,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true,
      "jsx": true,
      "experimentalObjectRestSpread": false
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jquery": true,
      "node": true
    },
    "rules": {
      "accessor-pairs": "error",
      "array-callback-return": "warn",
      "arrow-parens": "warn",
      "block-scoped-var": "error",
      "complexity": ["warn", 35],
      "class-methods-use-this": "off",
      "comma-dangle": ["warn", { "functions": "ignore", "objects": "always-multiline", "arrays": "always-multiline" }],
      "consistent-return": "off",
      "curly": ["error", "multi-line"],
      "default-case": "error",
      "dot-location": "off",
      "dot-notation": ["error", { "allowKeywords": true}],
      "eqeqeq": "error",
      "func-names": "off",
      "global-require": "off",
      "guard-for-in": "error",
      "import/extensions": ["warn", "never", { "json": "always", "less": "always" }],
      "import/newline-after-import": "warn",
      "import/no-dynamic-require": "warn",
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": "off",
      "indent": ["warn", 2, { "SwitchCase": 1 }],
      "jsx-quotes": ["warn", "prefer-single"],
      "key-spacing": "warn",
      "linebreak-style": "off",
      "new-cap": "error",
      "no-alert": "warn",
      "no-bitwise": "off",
      "no-caller": "error",
      "no-case-declarations": "warn",
      "no-cond-assign": ["error", "except-parens"],
      "no-console": "off",
      "no-continue": "warn",
      "no-div-regex": "error",
      "no-dupe-keys": "warn",
      "no-else-return": "error",
      "no-empty": ["error", { "allowEmptyCatch": true }],
      "no-eq-null": "error",
      "no-eval": "error",
      "no-extend-native": "error",
      "no-extra-bind": "error",
      "no-fallthrough": "error",
      "no-floating-decimal": "error",
      "no-implied-eval": "error",
      "no-irregular-whitespace": "error",
      "no-iterator": "error",
      "no-labels": "error",
      "no-lone-blocks": "error",
      "no-lonely-if": "error",
      "no-loop-func": "error",
      "no-mixed-operators": ["error", { "allowSamePrecedence": true }],
      "no-multi-spaces": "error",
      "no-multi-assign": "warn", // -> error
      "no-multi-str": "error",
      "no-native-reassign": "error",
      "no-nested-ternary": "error",
      "no-new": "off",
      "no-new-func": "error",
      "no-new-wrappers": "error",
      "no-octal": "error",
      "no-octal-escape": "error",
      "no-param-reassign": ["error", { "props": false }],
      "no-plusplus": "off",
      "no-process-env": "off",
      "no-proto": "error",
      "no-prototype-builtins": "error",
      "no-redeclare": "error",
      "no-return-assign": "error",
      "no-script-url": "error",
      "no-self-compare": "error",
      "no-sequences": "error",
      "no-shadow": "error",
      "no-throw-literal": "error",
      "no-undef": "error",
      "no-undef-init": "error",
      "no-underscore-dangle": "off",
      "no-unneeded-ternary": "error",
      "no-unreachable": "off",
      "no-unused-expressions": "error",
      "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
      "no-useless-call": "warn",
      "no-useless-concat": "warn",
      "no-useless-escape": "warn",
      "no-var": "error",
      "no-void": "warn",
      "no-warning-comments": ["off", { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
      "no-with": "error",
      "prefer-const": "warn", //2k
      "prefer-spread": "warn",
      "radix": "error",
      "semi": "warn",
      "space-before-blocks": "error",
      "space-before-function-paren": ["warn", "never"], // -> error
      "spaced-comment": "warn", // -> error
      "strict": ["warn", "global"],
      "wrap-iife": ["error", "any"],
      "yoda": "error",
      "quote-props": ["warn", "as-needed"],
      "prefer-destructuring": ["warn", { "array": false, "object": true }],
      "no-confusing-arrow": "error",
      "no-duplicate-imports": "error",
      "nonblock-statement-body-position": "error",
      "camelcase": "error",
      "padded-blocks": "warn", // -> error
      "prefer-template": "warn", // -> error
      "object-shorthand": ["warn", "properties"],
      "operator-assignment": "warn",
      "no-use-before-define": ["warn", { "functions": false }],
      "prefer-arrow-callback": "off",
      "prefer-rest-params": "off",
      "no-useless-return": "warn",
      "no-restricted-properties": "off",

      "react/jsx-closing-bracket-location": ["warn", "line-aligned"],
      "react/jsx-curly-spacing": "warn", // -> error
      "react/jsx-equals-spacing": "warn", // -> error
      "react/jsx-first-prop-new-line": "error",
      "react/jsx-indent": ["error", 2],
      "react/jsx-key": "error",
      "react/jsx-no-bind": "error",
      "react/jsx-no-duplicate-props": "error",
      "react/jsx-no-undef": "error",
      "react/jsx-sort-props": "off",
      "react/jsx-tag-spacing": "warn",
      "react/jsx-wrap-multilines": "error",
      "react/no-array-index-key": "warn",
      "react/no-danger": "off",
      "react/no-did-mount-set-state": "warn",
      "react/no-direct-mutation-state": "error",
      "react/no-find-dom-node": "off",
      "react/no-multi-comp": "off",
      "react/no-unknown-property": "warn",
      "react/prefer-es6-class": "warn", // -> error
      "react/prefer-stateless-function": "warn",
      "react/self-closing-comp": "warn",
      "react/require-render-return": "warn",
      "react/jsx-boolean-value": "warn",
      "react/jsx-no-target-blank": "warn",
      "react/no-unused-prop-types": "warn",
      "react/sort-comp": "warn",
      "react/require-default-props": "warn",
      "react/no-will-update-set-state": "warn",
      "react/jsx-max-props-per-line": ["warn", { "maximum": 2 }],
      "react/no-string-refs": "warn",
      "react/forbid-prop-types": "off",

      "jsx-a11y/html-has-lang": "warn",
      "jsx-a11y/lang": "warn",
      "jsx-a11y/no-access-key": "error",
      "jsx-a11y/anchor-has-content": "warn",
      "jsx-a11y/aria-props": "error",
      "jsx-a11y/aria-proptypes": "error",
      "jsx-a11y/aria-role": "error",
      "jsx-a11y/aria-unsupported-elements": "error",
      "jsx-a11y/heading-has-content": "error",
      "jsx-a11y/href-no-hash": "off", // REMOVED starting from 5.1.0
      "jsx-a11y/alt-text": "warn", // -> error
      "jsx-a11y/tabindex-no-positive": "error",
      "jsx-a11y/scope": "error",
      "jsx-a11y/iframe-has-title": "warn", // -> error
      "jsx-a11y/role-supports-aria-props": "warn",
      "jsx-a11y/role-has-required-aria-props": "warn",
      "jsx-a11y/no-onchange": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "warn",
      "jsx-a11y/no-static-element-interactions": "warn",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "jsx-a11y/label-has-for": "warn",
      "jsx-a11y/img-redundant-alt": "warn", // -> error
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      // TBD
      "no-restricted-syntax": "off", // Discuss
      "no-implicit-coercion": ["off", { "boolean": false }] // Discuss
    },
    "globals": {
      "_": true,
      "__webpack_public_path__": true,
      "adl": true,
      "Bloodhound": true,
      "bus": true,
      "constants": true,
      "createReactClass": true,
      "CSSRule": true,
      "globalLabels": true,
      "localize": true,
      "moment": true,
      "React": true,
      "ReactDOM": true,
      "Reflux": true,
      "replacePlaceholders": true,
      "sessionCache": true,
      "localCache": true,
      "Uri": true,
      //testing
      "setMockUrl": true,
      "renderer": true,
      "testSuite": true,
      "jest": true,
      "expect": true,
      "describe": true,
      "it": true,
      "beforeAll": true,
      "beforeEach": true,
      "afterAll": true,
      "afterEach": true,
      "mount": true,
      "shallow": true,
      "toJson": true,
      "PropTypes": true,
      "Async": true,
      "Loadable": true,
    }
  }
